{
    "routes": [
        {
            "src": "/",
            "dest": "/api/hello.js"
        },
        {
            "src": "/books",
            "dest": "/api/books.js"
        }
    ],
    "installCommand": "npm install",
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
